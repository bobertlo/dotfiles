XTerm*eightBitInput: false
XTerm*metaSendsEscape: true
XTerm*background: black
XTerm*foreground: lightgrey

Xft.dpi: 96
Xft.antialias: 1
Xft.autohint: 1
Xft.hinting: 1
Xft.hintstyle: hintfull
Xft.lcdfilter: lcddefault
Xft.rgba: rgb

URxvt.font: xft:liberationmono:pixelsize=12
URxvt.depth: 32
URxvt.background: [95]#1d1f21
URxvt.scrollBar: false

! special
*.foreground:   #c5c8c6
*.background:   #1d1f21
*.cursorColor:  #c5c8c6

! black
*.color0:       #1d1f21
*.color8:       #969896

! red
*.color1:       #cc342b
*.color9:       #cc342b

! green
*.color2:       #198844
*.color10:      #198844

! yellow
*.color3:       #fba922
*.color11:      #fba922

! blue
*.color4:       #3971ed
*.color12:      #3971ed

! magenta
*.color5:       #a36ac7
*.color13:      #a36ac7

! cyan
*.color6:       #39b8ed
*.color14:      #39c6ed

! white
*.color7:       #c5c8c6
*.color15:      #ffffff
